**
** And4 4 input AND logic object
** out = in1 && in2 && in3 && in4
**
class And4
extends Component
{
  @readonly property bool out
  property bool in1
  property bool in2

  property bool in3

  property bool in4

  virtual override void changed(Slot slot)
  {
    super.changed(slot);
    if(slot.name != "out")
      out = in1 && in2 && in3 && in4
  }

}
